.comp-desc-container {
  color: #333;
  padding-left: 5%;
  padding-right: 5%;
  width: 100%;
  box-sizing: border-box;
  line-height: 1.5rem;
  text-indent: 2rem;
}

.desc {
  width: 100%;
  box-sizing: border-box;
  padding-left: 5%;
  padding-right: 5%;
  text-indent: 2rem;
}

.text {
  color: orange;
  font-weight: bold;
}

.to-store {
  width: 100%;
  box-sizing: border-box;
  height: 30px;
  line-height: 30px;
  margin-top: 10px;
  text-align: center;
}

.comp-code-container {
  width: 100%;
  text-align: center;
  margin-top: 10px;
}

.tool-icon {
  width: 30px;
  height: 30px;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 0;
}

.tool-box {
  position: absolute;
  top: -30px;
  right: 0%;
  width: 70px;
  height: 120px;
  z-index: 999;
}

.box-container {
  position: absolute;
  top: 0%;
  left: 0%;
  height: 120px;
  width: 40px;
}

.box-container .ui-box {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 20px;
  width: 40px;
  height: 40px;
  background-color: #707070;
  color: white;
  z-index: 999;
}

.box-container .js-box {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 20px;
  width: 40px;
  height: 40px;
  background-color: rgba(255, 255, 0, 1);
  color: black;
  z-index: 999;
}

.box-container .cloud-box {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 20px;
  width: 40px;
  height: 40px;
  color: white;
  background-color: #1295dbe0;
  z-index: 999;
  font-family: Microsoft Yahei;
}