.compListContainer{
  display: flex;
  flex-direction: column;
  height: 100%;
  justify-content: space-around;
  padding: 100px 0;
}
.van-cell__title, .van-cell__value{
  font-size: 18px!important;
}

.van-cell__left-icon, .van-cell__right-icon {
  font-size: 20px!important;
}

.store-page-title{
  width: 100%;
  box-sizing: border-box;
  font-size: 22px;
  padding-left: 10%;
  padding-top: 40px;
}

.store-page-desc{
  width: 100%;
  padding-top: 10px;
  box-sizing: border-box;
  padding-left: 10%;
  font-size: 14px;
  padding-right: 4%;
  margin-bottom: 20px;
  color: #999;
}

.store-page-list{
  width: 90%;
  padding-left: 5%;
  margin-bottom: 5px;
}