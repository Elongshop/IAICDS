/**app.wxss**/
page {
  width: 100%;
  height: 100%;
  overflow-y: scroll;
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.container {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}

button {
  background: initial;
}

button:focus {
  outline: 0;
}

button::after {
  border: none;
}

image {
  width: 30px;
  height: 30px;
}
